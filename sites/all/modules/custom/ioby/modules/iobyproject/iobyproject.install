<?php

/**
 * Implements hook_schema()
 */
function iobyproject_schema() {
  $schema = array();

  $schema['iobyproject_totals_cache'] = array(
    'description' => 'Table to store the calculated total raised for projects.',
    'fields' => array(
      'nid' => array(
        'description' => 'The primary identifier for a node.',
        'type' => 'int',
        'unsigned' => TRUE,
        'not null' => TRUE,
      ),
      'total' => array(
        'description' => 'The calculated total amount for the project.',
        'type' => 'numeric',
        'precision' => 10,
        'scale' => 2,
        'unsigned' => TRUE,
        'not null' => TRUE,
        'default' => 0,
      ),
      'changed' => array(
        'description' => 'The Unix timestamp when the cache was most recently updated.',
        'type' => 'int',
        'not null' => TRUE,
        'default' => 0,
      ),
    ),
    'primary key' => array('nid'),
  );

  $schema['iobyproject_funding_notifications'] = array(
    'description' => 'Table to log which budget/funding threshold notifications have been sent.',
    'fields' => array(
      'nid' => array(
        'description' => 'The node id of the project.',
        'type' => 'int',
        'unsigned' => TRUE,
        'not null' => TRUE,
      ),
      'next_notification_threshold' => array(
        'type' => 'int',
        'not null' => TRUE,
        'default' => 0,
        'size' => 'tiny',
        'description' => 'The next threshold level to send a notification at.',
      ),
      'changed' => array(
        'description' => 'The Unix timestamp when the table was last updated.',
        'type' => 'int',
        'not null' => TRUE,
        'default' => 0,
      ),
    ),
    'primary key' => array('nid'),
  );

  $schema['iobyproject_node_extra'] = array(
    'description' => 'Table to attach extra metadata to project_2 nodes.',
    'fields' => array(
      'nid' => array(
        'description' => 'The node id of the project.',
        'type' => 'int',
        'unsigned' => TRUE,
        'not null' => TRUE,
      ),
      'project_form_version' => array(
        'description' => 'The version of the project form that this project is using.',
        'type' => 'int',
        'size' => 'tiny',
        'unsigned' => TRUE,
        'not null' => TRUE,
        'default' => 2,
      ),
    ),
    'primary key' => array('nid'),
    'indexes' => array(
      'project_form_version' => array('project_form_version'),
    ),
  );

  return $schema;
}

/**
 * Add and populate the iobyproject_totals_cache table
 */
function iobyproject_update_7000() {
  $schema = iobyproject_schema();
  db_create_table('iobyproject_totals_cache', $schema['iobyproject_totals_cache']);

  // Tally the totals and update the table
  update_project_totals_cache();
}

/**
 * Add the iobyproject_funding_notifications table
 */
function iobyproject_update_7001() {
  $schema = iobyproject_schema();
  if (!db_table_exists('iobyproject_funding_notifications')) {
    db_create_table('iobyproject_funding_notifications', $schema['iobyproject_funding_notifications']);
  }
}

/**
 * Upgrade to version 2 of the project form
 */
function iobyproject_update_7002() {
  $schema = iobyproject_schema();

  // Create and populate iobyproject_node_extra
  if (!db_table_exists('iobyproject_node_extra')) {
    db_create_table('iobyproject_node_extra', $schema['iobyproject_node_extra']);
  }
  db_query("INSERT INTO {iobyproject_node_extra} (nid, project_form_version) SELECT n.nid, 1 FROM {node} n WHERE n.type = 'project_2'");

  // Convert project_start_date field to remove todate
  $start_date_settings = unserialize(db_select('field_config', 'f')
    ->fields('f', array('data'))
    ->condition('f.field_name', 'field_project_start_date', '=')->execute()->fetchField());

  $start_date_settings['settings']['todate'] = NULL;
  unset($start_date_settings['storage']['details']['sql']['FIELD_LOAD_CURRENT']['field_data_field_project_start_date']['value2']);
  unset($start_date_settings['storage']['details']['sql']['FIELD_LOAD_REVISION']['field_revision_field_project_start_date']['value2']);

  db_update('field_config')
    ->fields(array('data' => serialize($start_date_settings)))
    ->condition('field_name', 'field_project_start_date', '=')
    ->execute();

  db_drop_field('field_data_field_project_start_date', 'field_project_start_date_value2');
  db_drop_field('field_revision_field_project_start_date', 'field_project_start_date_value2');

  // Enable new modules
  module_enable(array(
    'mathfield',
    'twitter_username',
    'bundle_copy',
