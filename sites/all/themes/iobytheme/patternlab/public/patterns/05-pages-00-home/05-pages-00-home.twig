{% extends 'wrappers-home' %}

{% block home_top %}
  {% include 'organisms-hero' with { post : hero } %}
{% endblock %}

{% block home_outline_first %}
  {% include 'organisms-impact-list' with { post : impacts } %}
{% endblock %}

{% block home_middle %}
  {% include 'organisms-connect' %}
  {% include 'organisms-featured-projects' with { post : projects } %}
  {% include 'organisms-cities' with { post : cities } %}
  {% if projecttypes %}
    {% include 'organisms-find-projects' with { post : projecttypes } %}
  {% endif %}
  {% include 'organisms-more-projects' with { post : more } %}

{% endblock %}

{% block home_outline_last %}
  {% include 'organisms-updates' with { post : updates } %}
  {% include 'organisms-promo-list' with { post : promos } %}
{% endblock %}
